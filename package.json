{
  "name": "graphql-codegen-webpack-plugin",
  "version": "0.1.0",
  "description": "GraphQL CodeGen Webpack Plugin",
  "main": "./lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/*",
    "build": "tsc -p .",
    "watch": "tsc -w",
    "lint": "tslint 'src/*.ts*' -p tsconfig.json",
    "tests": "jest",
    "tests:watch": "jest --watch",
    "test": "yarn tests && yarn lint",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysgears/graphql-codegen-webpack-plugin.git"
  },
  "keywords": [
    "apollo",
    "graphql",
    "codegen"
  ],
  "author": "Victor Vlasenko <victor.vlasenko@sysgears.com> (https://github.com/sysgears)",
  "license": "MIT",
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/sysgears/graphql-codegen-webpack-plugin/issues"
  },
  "homepage": "https://github.com/sysgears/graphql-codegen-webpack-plugin",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/memory-fs": "^0.3.2",
    "@types/node": "^7.0.18",
    "@types/tmp": "^0.0.33",
    "@types/webpack": "^4.4.17",
    "graphql": "^14.0.2",
    "graphql-code-generator": "0.13.0-alpha.fa3dd811",
    "graphql-codegen-typescript-template": "0.13.0-alpha.fa3dd811",
    "graphql-tag": "^2.10.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^4.1.3",
    "memory-fs": "^0.4.1",
    "prettier": "^1.6.1",
    "rimraf": "^2.6.2",
    "tmp": "^0.0.33",
    "ts-jest": "^23.10.1",
    "tslint": "^5.2.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.1.3",
    "webpack": "^4.23.1",
    "webpack-sources": "^1.3.0"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "parser": "babylon"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {},
  "peerDependencies": {}
}
